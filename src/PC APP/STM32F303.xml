<?xml version="1.0" ?>
<ConfigData>
  <processor>STM32F303</processor>
  <core>ARM cortex M3</core>
  <clock>72Mhz</clock>
  <comm>UART/USB</comm>
  <connect>TX - PD8, RX - PD9</connect>
  <scope>
    <sc_maxf>1000000</sc_maxf>
    <sc_depth>12</sc_depth>
    <sc_v_ref>3000</sc_v_ref>
    <sc_buffsize>500</sc_buffsize>
    <sc_pin>PC1</sc_pin>
  </scope>
  <log_analyser>
    <la_maxf>100000</la_maxf>
    <la_bus_lnght>8</la_bus_lnght>
    <la_5tolerant>YES</la_5tolerant>
    <la_pins>PD 8-15</la_pins>
  </log_analyser>
  <generator>
    <gen_maxf>20000000</gen_maxf>
    <gen_depth>12</gen_depth>
    <gen_v_ref>3000</gen_v_ref>
    <gen_buffsize>500</gen_buffsize>
    <gen_pin>PA4</gen_pin>
  </generator>
</ConfigData>
